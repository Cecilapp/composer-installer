{
  "name": "cecil/installer",
  "description": "Installer for Cecil's themes and extensions.",
  "type": "composer-plugin",
  "keywords": [
    "cecil",
    "installer",
    "themes",
    "extensions"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Arnaud Ligny",
      "email": "arnaud@ligny.fr",
      "homepage": "https://arnaudligny.fr",
      "role": "Developer"
    }
  ],
  "require": {
    "composer-plugin-api": ">=2.0"
  },
  "require-dev": {
    "composer/composer": ">=2.0"
  },
  "autoload": {
    "psr-0": {
      "Cecil\\Installer": "src/"
    }
  },
  "extra": {
    "class": "Cecil\\Installer\\Plugin",
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
